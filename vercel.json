{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [ { "type": "host", "value": ".*\\.vercel\\.app" } ],
      "destination": "https://promptready.app/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "$ROBOTS_TAG_VALUE"
        }
      ]
    }
  ]
}